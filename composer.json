{
  "name": "baks-dev/field-pack",
  "description": "Пакет дефолтных полей HTML",
  "homepage": "https://github.com/baks-dev/field-pack",
  "keywords": [
    "bundle",
    "symfony",
    "library",
    "reference",
    "field",
    "pack",
    "baks"
  ],
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Baks Development",
      "email": "admin@baks.dev"
    }
  ],
  "require": {
    "php": ">=8.4",
    "baks-dev/core": "^7.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "BaksDev\\Field\\Pack\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BaksDev\\Field\\Pack\\Tests\\": "Tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
